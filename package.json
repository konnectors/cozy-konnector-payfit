{
  "name": "cozy-konnector-payfit",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-payfit.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "renovate": {
    "extends": ["config:base"],
    "patch": {
      "automerge": true
    }
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "eslintIgnore": ["build"],
  "scripts": {
    "start": "node src/index.js",
    "test": "konitor testit .",
    "dev": "cozy-konnector-dev src/index.js",
    "standalone": "cozy-konnector-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-konnector-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-konnector-standalone src/index.js",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,json}'",
    "precommit": "eslint . && pretty-quick --staged",
    "deploy:travis":
      "git-directory-deploy --username Cozy --email contact@cozycloud.cc --directory build/ --repo=https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-payfit.git",
    "deploy":
      "git-directory-deploy --directory build/ --branch build --repo=https://github.com/konnectors/cozy-konnector-payfit.git",
    "deploy:prod":
      "git-directory-deploy --directory build/ --branch latest --repo=https://github.com/konnectors/cozy-konnector-payfit.git"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.5.3"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "eslint": "4.18.1",
    "eslint-config-prettier": "2.9.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "konitor": "0.5.3",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.13"
  }
}
